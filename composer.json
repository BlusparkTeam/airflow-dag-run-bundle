{
  "name": "bluspark/airflow-dag-run-bundle",
  "description": "Provide an HTTP Client to manage DAG run from Airflow",
  "type": "symfony-bundle",
  "license": "CECILL-B",
  "autoload": {
    "psr-4": {
      "Bluspark\\AirflowDagRunBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bluspark\\AirflowDagRunBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Bluspark"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/http-kernel": "^6.3",
    "symfony/config": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/http-client": "^6.3",
    "symfony/scheduler": "^6.3",
    "symfony/messenger": "^6.3",
    "symfony/event-dispatcher": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^1.10"
  }
}
